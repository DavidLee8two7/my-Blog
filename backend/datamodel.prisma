type User {
  id: ID! @unique
  name: String!
  email: String!
}

type Blog {
  id: ID! @unique
  title: String!
  subject: String!
  message: String!
  image: String
  largeImage: String
  # user: User!
}

type Memo {
  id: ID! @unique
  name: String!
  email: String!
  subject: String!
  message: String!
  # user: User!
}
